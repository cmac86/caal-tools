  "name": "truenas-get-status",
  "version": "1.0.0",
  "description": "Gets TrueNAS system status including CPU, memory, storage pools, and running apps.",
  "category": "homelab",
  "voice_triggers": [
    "What's my TrueNAS status?",
    "Check my NAS"
  ],
  "required_services": [
    "truenas"
  ],
  "required_credentials": [
    {
      "credential_type": "httpHeaderAuth",
      "name": "HTTPHEADERAUTH_CREDENTIAL",
      "description": "n8n credential type: httpHeaderAuth"
    }
  ],
  "required_variables": [
    {
      "name": "TRUENAS_URL",
      "description": "Your TrueNAS server URL (IP and port)",
      "example": "http://192.168.1.100:80"
    }
  ],
  "author": {
    "github": "cmac86"
  },
